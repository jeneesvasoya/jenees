                <div class="w3ls-bt-ft">
                    <div class="navigation-w3ls">
                        <nav class="navbar-btm">
                            <div class="logo">
                                <h3><a class="navber-brand" href="index.html"><i class="fa fa-heartbeat" aria-hidden="true"></i>MYDOCTORFINDER</a></h3>
                            </div>
                            <div class="footer-nav">
                                <ul class="nav1 w3ls-ma">
                                    <li class="active"><a data-hover="Home" class="scroll" href="#home">Home</a></li>
                                    <li><a data-hover="appointment" href="#appointment" class="scroll">Appointment</a></li>
                                    <li><a data-hover="OurStaff" href="#staff" class="scroll">OurStaff</a></li>
                                    <li><a data-hover="ourlab" href="#grid-gallery" class="scroll">Our lab</a></li>
                                    <li><a data-hover="Contact" href="#contact" class="scroll">Contact</a></li>
                                </ul>
